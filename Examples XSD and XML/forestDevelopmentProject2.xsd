<?xml version="1.0" encoding="utf-8" ?>
<!--Created with Liquid XML 2013 Designer Edition 11.1.0.4725 (http://www.liquid-technologies.com)-->
<xs:schema xmlns:ns0="http://rosleshoz.gov.ru/xmlns/cTypes/4.2"
           xmlns:ns2="http://rosleshoz.gov.ru/xmlns/sTypes/4.1"
           elementFormDefault="qualified"
           targetNamespace="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.2"
           version="4.2"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:import schemaLocation="http://rosleshoz.gov.ru/xmlns/cTypes/4.2/cTypes.xsd"
               namespace="http://rosleshoz.gov.ru/xmlns/cTypes/4.2" />
    <xs:import schemaLocation="http://rosleshoz.gov.ru/xmlns/sTypes/4.1/sTypes.xsd"
               namespace="http://rosleshoz.gov.ru/xmlns/sTypes/4.1" />
    <xs:element xmlns:q1="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.2"
                name="forestDevelopmentProject"
                type="q1:forestDevelopmentProject">
        <xs:annotation>
            <xs:documentation>Корневой элемент - "Проект освоения лесов"</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="forestDevelopmentProject">
        <xs:annotation>
            <xs:documentation>Описание проекта освоения лесов</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="serviceInfo"
                        type="ns0:serviceInfo" />
            <xs:element name="preamble">
                <xs:annotation>
                    <xs:documentation>Преамбула - титульный лист</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="location"
                                    type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Описание местоположения лесного участка</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element xmlns:q1="http://rosleshoz.gov.ru/xmlns/sTypes/4.1"
                                    name="totalArea"
                                    type="q1:area">
                            <xs:annotation>
                                <xs:documentation>Площадь лесного участка, га</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="grounds"
                                    type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Основание для предоставления лесного участка</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="usageType"
                                    type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Описание видов использования лесов</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="contract"
                                    type="ns0:contract">
                            <xs:annotation>
                                <xs:documentation>Описание правоустанавливающего документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="general">
                <xs:annotation>
                    <xs:documentation>I. Общие сведения</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="partner"
                                    type="ns0:partner">
                            <xs:annotation>
                                <xs:documentation>Сведения о лице, использующем лесной участок</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="usageType"
                                    type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Описание видов использования лесов</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="address"
                                    type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Адрес юридического или физического лица</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="contactInformation"
                                    type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Контактная информация</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="contract"
                                    type="ns0:contract">
                            <xs:annotation>
                                <xs:documentation>Описание правоустанавливающего документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="usePeriod"
                                    type="xs:float">
                            <xs:annotation>
                                <xs:documentation>Срок использования, лет</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="executiveAuthority"
                                    type="ns0:reference">
                            <xs:annotation>
                                <xs:documentation>Сведения об органе государственной власти или органе местного самоуправления, предоставившем лесной участок в аренду или постоянное (бессрочное) пользование, установившем сервитут или предусмотренный статьей 39.37 Земельного кодекса Российской Федерации, публичный сервитут
</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="regulations"
                                    type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Перечень законодательных и иных нормативно-правовых актов, нормативно-технических, методических и проектных документов, на основе которых разработан проект освоения лесов</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="validityPeriod"
                                    type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Срок действия проекта освоения лесов</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="developer"
                                    type="ns0:signerData">
                            <xs:annotation>
                                <xs:documentation>Разработчик проекта</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="yearForestManagement"
                                    type="ns2:year">
                            <xs:annotation>
                                <xs:documentation>Год последнего лесоустройства</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="info">
                            <xs:annotation>
                                <xs:documentation>Сведения о лесном участке</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="listQuartersTaxationUnit">
                                        <xs:annotation>
                                            <xs:documentation>Перечень предоставленных в аренду, постоянное(бессрочное) пользование, в отношении которых установлен сервитут или публичный сервитут, лесных кварталов, лесотаксационных выделов или их частей</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element xmlns:q5="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.1"
                                                            name="row"
                                                            minOccurs="0"
                                                            maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="location"
                                                                        type="ns0:location">
                                                                <xs:annotation>
                                                                    <xs:documentation>В поле taxationUnit может присутствовать только одно значение (без перечисления через запятую или тире</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="area"
                                                                        type="ns2:area">
                                                                <xs:annotation>
                                                                    <xs:documentation>Площадь эксплуатационная</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="number"
                                                          type="xs:string"
                                                          default="1">
                                                <xs:annotation>
                                                    <xs:documentation>Номер таблицы</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="distributionSpecialPurpose">
                                        <xs:annotation>
                                            <xs:documentation>Распределение площади лесного участка по видам целевого назначения лесов на защитные (по их категориям), эксплуатационные и резервные леса</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element xmlns:q5="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.1"
                                                            name="row"
                                                            minOccurs="0"
                                                            maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="specialPurpose"
                                                                        type="ns0:reference">
                                                                <xs:annotation>
                                                                    <xs:documentation>Целевое назначение лесов</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="protectionCategory"
                                                                        type="ns0:reference"
                                                                        minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Категория защитных лесов</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="area"
                                                                        type="ns2:area">
                                                                <xs:annotation>
                                                                    <xs:documentation>Площадь, га</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="percent"
                                                                        type="ns2:percent">
                                                                <xs:annotation>
                                                                    <xs:documentation>Процент</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="number"
                                                          type="xs:string"
                                                          default="1">
                                                <xs:annotation>
                                                    <xs:documentation>Номер таблицы</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="averageTaxationRates">
                                        <xs:annotation>
                                            <xs:documentation>Таксационная характеристика лесных насажденийна лесном участке</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element xmlns:q5="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.1"
                                                            name="row"
                                                            minOccurs="0"
                                                            maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="specialPurpose"
                                                                        type="ns0:reference">
                                                                <xs:annotation>
                                                                    <xs:documentation>Целевое назначение лесов</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="farm"
                                                                        type="ns2:farm">
                                                                <xs:annotation>
                                                                    <xs:documentation>Справочник "Хозяйства"</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="tree"
                                                                        type="ns0:reference">
                                                                <xs:annotation>
                                                                    <xs:documentation>Преобладающая порода (Указывается элемент справочника "Породы древесины")</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="area"
                                                                        type="ns2:area">
                                                                <xs:annotation>
                                                                    <xs:documentation>Площадь, га</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="age"
                                                                        type="xs:int">
                                                                <xs:annotation>
                                                                    <xs:documentation>Возраст насаждения</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="bonitet"
                                                                        type="ns0:reference">
                                                                <xs:annotation>
                                                                    <xs:documentation>Бонитет</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="completeness"
                                                                        type="xs:decimal">
                                                                <xs:annotation>
                                                                    <xs:documentation>Полнота насаждения</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="volume"
                                                                        type="ns2:valueCutting">
                                                                <xs:annotation>
                                                                    <xs:documentation>Средний запас насаждений покрытых лесной растительностью на 1 га, м.куб</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="ripeAndOverripe"
                                                                        type="ns2:valueCutting">
                                                                <xs:annotation>
                                                                    <xs:documentation>Средний запас спелых и перестойных насаждений на 1 га, м.куб</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="growth"
                                                                        type="ns2:valueCutting">
                                                                <xs:annotation>
                                                                    <xs:documentation>Средний прирост по запасу на 1 га, м.куб</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="composition"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Состав насаждения</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="number"
                                                          type="xs:string"
                                                          default="1">
                                                <xs:annotation>
                                                    <xs:documentation>Номер таблицы</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="speciallyProtectedNaturalAreas"
                                                minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Характеристика имеющихся в границах лесного участка особо охраняемых природных территорий и объектов (границы и режим особой охраны), мероприятия по сохранению объектов биоразнообразия</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element xmlns:q5="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.1"
                                                            name="row"
                                                            minOccurs="0"
                                                            maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="speciallyProtectedNaturalArea"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Особо охраняемая природная территория</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="securityMode"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Режим охраны</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="event"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Мероприятия по сохранениию объектов</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="location">
                                                                <xs:annotation>
                                                                    <xs:documentation>Расположение ООПТ</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:complexType>
                                                                    <xs:sequence>
                                                                        <xs:element name="row"
                                                                                    type="ns0:location"
                                                                                    minOccurs="1"
                                                                                    maxOccurs="unbounded" />
                                                                    </xs:sequence>
                                                                </xs:complexType>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="number"
                                                          type="xs:string"
                                                          default="1">
                                                <xs:annotation>
                                                    <xs:documentation>Номер таблицы</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="contamination"
                                                minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Сведения о наличии загрязнения лесов (в том числе нефтяного, радиоактивного)</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element xmlns:q5="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.1"
                                                            name="row"
                                                            minOccurs="0"
                                                            maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="location"
                                                                        type="ns0:location">
                                                                <xs:annotation>
                                                                    <xs:documentation />
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="area"
                                                                        type="ns2:area">
                                                                <xs:annotation>
                                                                    <xs:documentation>Площадь, га</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="typeContamination"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Вид загрязнения</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="unitType"
                                                                        type="ns0:reference">
                                                                <xs:annotation>
                                                                    <xs:documentation>Единица измерения</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="volume"
                                                                        type="xs:float">
                                                                <xs:annotation>
                                                                    <xs:documentation>Количественные показатели загрязнения</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="number"
                                                          type="xs:string"
                                                          default="1">
                                                <xs:annotation>
                                                    <xs:documentation>Номер таблицы</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="rarePlantsAnimals"
                                                minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Сведения о наличии мест обитания редких и находящихся под угрозой исчезновения видов животных и мест произрастания редких и находящихся под угрозой исчезновения видов деревьев, кустарников, лиан и иных лесных растений</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element xmlns:q5="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.1"
                                                            name="row"
                                                            minOccurs="0"
                                                            maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="species"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Вид, порода</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="restrictions"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Ограничения</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="basisProtection"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Основание для охраны</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="location">
                                                                <xs:annotation>
                                                                    <xs:documentation>Расположение ООПТ</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:complexType>
                                                                    <xs:sequence>
                                                                        <xs:element name="row"
                                                                                    type="ns0:location"
                                                                                    minOccurs="1"
                                                                                    maxOccurs="unbounded" />
                                                                    </xs:sequence>
                                                                </xs:complexType>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="number"
                                                          type="xs:string"
                                                          default="1">
                                                <xs:annotation>
                                                    <xs:documentation>Номер таблицы</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="surveys"
                                                minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Сведения о материалах специальных изысканий, исследований или иных специальных обследований (при наличии)</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element xmlns:q5="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.1"
                                                            name="row"
                                                            minOccurs="0"
                                                            maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="surveyMaterials"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Материалы изыскания или обследования</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="requisites"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>реквизиты</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="number"
                                                          type="xs:string"
                                                          default="1">
                                                <xs:annotation>
                                                    <xs:documentation>Номер таблицы</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="encumbrances"
                                                minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Сведения об обременениях лесного участка</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element xmlns:q5="http://rosleshoz.gov.ru/xmlns/forestDevelopmentProject/4.1"
                                                            name="row"
                                                            minOccurs="0"
                                                            maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="typeEncumbrance"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>Вид обременения</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="borderArea"
                                                                        type="xs:string">
                                                                <xs:annotation>
                                                                    <xs:documentation>реквизиты</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="number"
                                                          type="xs:string"
                                                          default="1">
                                                <xs:annotation>
                                                    <xs:documentation>Номер таблицы</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="objects">
                            <xs:annotation>
